@{Layout = null;}
<div class="modal fade" id="modal-clasificacion-buscar" tabindex="-1" aria-labelledby="titulo-clasificacion" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header bg-light p-2">
                <h5 class="modal-title" id="titulo-clasificacion">BUSCAR CLASIFICACIÓN</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead class="table-light">
                            <tr>
                                <th style="width:30px"></th>
                                <th>Tipo de Inspección</th>
                                <th>Sub Tipos de Inspección</th>
                            </tr>
                        </thead>
                        <tbody id="list-clasificacion">
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <div class="hstack gap-2 justify-content-end">
                    <button type="button" class="btn btn-danger" data-bs-dismiss="modal" id="btn-modal-clasificacion-cerrar"><i class="ri-close-fill align-bottom"></i>Cerrar</button>
                    <button type="button" class="btn btn-success" id="btn-modal-clasificacion-aceptar"><i class="ri-check-fill align-bottom"></i>Aceptar</button>
                </div>
            </div>
        </div>
    </div>
</div>